{
  "name": "typing-trainer-landing",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "vue:lint": "eslint . --ext .vue",
    "vue:lint:fix": "eslint . --ext .vue --fix",
    "ts:lint": "eslint . --ext .ts",
    "ts:lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check \"./**/*.{js,ts,vue}\"",
    "format:fix": "prettier --write \"./**/*.{js,ts,vue}\"",
    "code:lint": "npm run lint && npm run vue:lint && npm run format",
    "code:fix": "npm run format:fix && npm run lint:fix && npm run vue:lint:fix",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "nuxt": "^4.2.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "autoprefixer": "^10.4.21",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.3"
  }
}
